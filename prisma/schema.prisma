generator client {
  provider = "prisma-client-js"
}

// 指定数据库
datasource db {
  provider = "mongodb"
  url      = env("MONGODB_DATABASE_URL")
}


model User {
  id        Int     @default(autoincrement()) @id
  name      String
  password  String
}
