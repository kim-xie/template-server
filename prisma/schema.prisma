generator client {
  provider = "prisma-client-js"
}

// 指定数据库
datasource db {
  provider = "mongodb"
  url      =  env("MONGODB_DATABASE_URL")
}


model User {
  id          String        @id @default(cuid())  @map("_id") // 使用 cuid() 作为唯一标识符
  name        String
  password    String
  createdAt   DateTime      @default(now())
  updatedAt   DateTime      @updatedAt
}
